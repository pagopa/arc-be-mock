          "payer": {
            "$ref": "#/components/schemas/UserDetail"
          },
          "amount": {
            "type": "string"
          },
          "fee": {
            "type": "string"
          },
          "origin": {
            "type": "string",
            "enum": [
              "INTERNAL",
              "PM",
              "NDP001PROD",
              "NDP002PROD",
              "NDP003PROD",
              "UNKNOWN"
            ]
          }
        }
